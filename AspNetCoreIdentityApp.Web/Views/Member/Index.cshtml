@{
  ViewData["Title"] = "Index";
}
@model UserViewModel



<div class="row">
  <div class="col-md-12 ">
    <h1>Kullanıcı Bilgileri</h1>  

    <table class="table">
      <thead>
        <tr>
          <th>Profil Resmi</th>
          <th>Kullanıcı Adı</th>
          <th>Email</th>
          <th>Telefon No</th>
          <th>Cinsiyet</th>
          <th>Doğum Tarihi</th>
          <th>Şehir</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><user-picture-thumbnail class="rounded" style="width:50px" picture-url="@Model.PictureUrl" /></td>
          <td>@Model.UserName</td>
          <td>@Model.Email</td>
          <td>@Model.PhoneNumber</td>
          <td>@Model.Gender</td>
          <td>@Model.BirthDate</td>
          <td>@Model.City</td>
        </tr>
      </tbody>
    </table>

  </div>
</div>





